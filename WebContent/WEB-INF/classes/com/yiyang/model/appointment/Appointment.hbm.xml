<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.yiyang.model.appointment">
      <class name="Appointment" table="tb_appointment">
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="createTime"/>
		<property name="realName" length="20"/>
		<property name="address" length="200"/>
		<property name="occupy" length="50"/>
		<property name="mobile" length="11"/>
		<property name="requirement" type="text"/>
	</class>
</hibernate-mapping>